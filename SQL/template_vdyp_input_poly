    PRE_DISTURBANCE_STOCKABILITY double precision,
    YIELD_FACTOR double precision,
    NON_PRODUCTIVE_DESCRIPTOR_CD text COLLATE pg_catalog."default",
    BCLCS_LEVEL1_CODE text COLLATE pg_catalog."default",
    BCLCS_LEVEL2_CODE text COLLATE pg_catalog."default",
    BCLCS_LEVEL3_CODE text COLLATE pg_catalog."default",
    BCLCS_LEVEL4_CODE text COLLATE pg_catalog."default",
    BCLCS_LEVEL5_CODE text COLLATE pg_catalog."default",
    PHOTO_ESTIMATION_BASE_YEAR integer,
    REFERENCE_YEAR integer,
