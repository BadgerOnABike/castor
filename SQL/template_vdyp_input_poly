    NON_VEG_COVER_TYPE_1 text COLLATE pg_catalog."default",
    NON_VEG_COVER_PCT_1 integer,
    NON_VEG_COVER_PATTERN_1 integer,
    NON_VEG_COVER_TYPE_2 text COLLATE pg_catalog."default",
    NON_VEG_COVER_PCT_2 integer,
    NON_VEG_COVER_PATTERN_2  text COLLATE pg_catalog."default",
    NON_VEG_COVER_TYPE_3  text COLLATE pg_catalog."default",
    NON_VEG_COVER_PCT_3 integer,
    NON_VEG_COVER_PATTERN_3  text COLLATE pg_catalog."default",
    LAND_COVER_CLASS_CD_1 text COLLATE pg_catalog."default",
    LAND_COVER_PCT_1 integer,
    LAND_COVER_CLASS_CD_2 text COLLATE pg_catalog."default",
    LAND_COVER_PCT_2 integer,
